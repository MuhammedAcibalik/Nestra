{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "./dist/test",
        "types": [
            "jest",
            "node"
        ],
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "include": [
        "src/**/*.spec.ts",
        "src/**/*.test.ts",
        "test/**/*.ts",
        "src/**/*.d.ts"
    ]
}